extends layout
block body
    div(class="container", ng-controller="HistoryCtrl")
        h1 GitOpen
        nav(class="navbar navbar-default",role="navigation")
            form(role="search", class="navbar-form navbar-left")
                div(class="form-group")
                    label(for="dropdown-year") Velg Ã¥r
                    select(id="dropdown-year", class="input-sm", ng-model="year", ng-options="year for year in [2014,2013,2012,2011,2010]")
            form(role="search", class="navbar-form navbar-left")
                div(class="form-group")
                    label(for="dropdown-chart") Velg graf
                    select(id="dropdown-chart", class="input-sm", ng-model="chart", ng-options="chart for chart in ['Bubble', 'Spline']")
        div(class="row")
            div(class="span9")
                h6(class="widget-title")
                    highchart(id="chart1",config="chartConfig")
        div(class="row")
            div(class="span9")
                a(href="#") BACK

