extends layout
block body
    div(class="container", ng-controller="HistoryCtrl")
        h1 GitOpen
        ol(class="ab-nab breadcrumb")
            li(ng-repeat="breadcrumb in breadcrumbs.get() track by breadcrumb.path",ng-class="{ active: $last }")
                a(ng-if="!$last" ng-href="#{{ breadcrumb.path }}",ng-bind="breadcrumb.label",class="margin-right-xs")
                span(ng-if="$last",ng-bind="breadcrumb.label")
        nav(class="navbar navbar-default",role="navigation")
            div(class="navbar-header")
                a(class="navbar-brand") Velg Ã¥r
            form(role="search", class="navbar-form navbar-left")
                div(class="form-group")
                    select(id="dropdown-year", class="input-sm form-control", ng-model="year", ng-options="year for year in [2014,2013,2012,2011,2010]")
            div(class="navbar-header")
                a(class="navbar-brand") Velg graf
            form(role="search", class="navbar-form navbar-left")
                div(class="form-group")
                    select(id="dropdown-chart", class="input-sm form-control", ng-model="chart", ng-options="chart for chart in ['Bubble', 'Spline']")
        div(class="row")
            div(class="span9")
                h6(class="widget-title")
                    highchart(id="chart1",config="chartConfig")
        div(class="row")
            div(class="span9")
                a(href="#") BACK

    script(src='components/angular/angular.js')
    script(src="http://code.highcharts.com/highcharts.js")
    script(src="http://code.highcharts.com/highcharts-more.js")
    //script(src="http://code.highcharts.com/modules/exporting.js")
    script(src="https://rawgithub.com/pablojim/highcharts-ng/master/dist/highcharts-ng.js")
    script(src='components/angular-route/angular-route.js')
    script(src='components/angular-resource/angular-resource.js')
    script(src='components/toastr/toastr.js')
    script(src="components/angular-socket-io/socket.js")
    script(src='js/app.js')
    script(src='js/services.js')
    script(src='js/controllers.js')
    script(src='js/filters.js')
    script(src='js/directives.js')
    script(src='components/underscore/underscore.js')
    script(src='http://code.jquery.com/ui/1.9.2/jquery-ui.js')
    script(src='components/ng-breadcrumbs/dist/js/ng-breadcrumbs.js')